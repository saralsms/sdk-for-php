{
  "name": "saralsms/sdk-for-php",
  "type": "library",
  "description": "SaralSMS SDK for the PHP programming language.",
  "homepage": "https://saralsms.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "SaralSMS",
      "homepage": "https://saralsms.com"
    }
  ],
  "support": {
    "email": "support@saralsms.com",
    "homepage": "https://saralsms.com"
  },
  "keywords": [
    "api",
    "client",
    "gateway",
    "saralsms",
    "sdk",
    "service",
    "sms",
    "verification"
  ],
  "require": {
    "php": "^7.2",
    "ext-curl": "*",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^6.5",
    "guzzlehttp/promises": "^1.3",
    "guzzlehttp/psr7": "^1.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {},
  "autoload": {
    "psr-4": {
      "SaralSMS\\": "src/"
    },
    "classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {}
}
